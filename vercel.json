{
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "api/auth/signup.ts": {
      "runtime": "@vercel/node@3"
    },
    "api/auth/signin.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}